{
  "name": "@microsoft/terraform-cdk-constructs",
  "description": "Azure CDK constructs using AZAPI provider for direct Azure REST API access. Version 1.0.0 - Major breaking change migration from AzureRM to AZAPI.",
  "repository": {
    "type": "git",
    "url": "https://github.com/azure/terraform-cdk-constructs.git"
  },
  "scripts": {
    "build": "npx projen build",
    "bump": "npx projen bump",
    "clobber": "npx projen clobber",
    "compat": "npx projen compat",
    "compile": "npx projen compile",
    "default": "npx projen default",
    "docgen": "npx projen docgen",
    "eject": "npx projen eject",
    "eslint": "npx projen eslint",
    "package": "npx projen package",
    "package-all": "npx projen package-all",
    "package:dotnet": "npx projen package:dotnet",
    "package:java": "npx projen package:java",
    "package:js": "npx projen package:js",
    "package:python": "npx projen package:python",
    "post-compile": "npx projen post-compile",
    "post-upgrade": "npx projen post-upgrade",
    "pre-compile": "npx projen pre-compile",
    "release": "npx projen release",
    "test": "jest --testMatch '**/*.spec.ts'",
    "test:update": "npx projen test:update",
    "test:watch": "npx projen test:watch",
    "unbump": "npx projen unbump",
    "upgrade": "npx projen upgrade",
    "watch": "npx projen watch",
    "projen": "npx projen",
    "integration": "STREAM_OUTPUT=true jest --testMatch '**/*.integ.ts'",
    "integration:nostream": "STREAM_OUTPUT=false jest --maxWorkers=12 --testMatch '**/*.integ.ts'",
    "generate-index": "node ./scripts/generate-index.js"
  },
  "author": {
    "name": "Microsoft",
    "url": "https://microsoft.com",
    "organization": false
  },
  "devDependencies": {
    "@types/jest": "^27",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "cdktf": "0.20.8",
    "constructs": "10.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^27",
    "jest-junit": "^15",
    "jsii": "~5.9.0",
    "jsii-diff": "^1.98.0",
    "jsii-docgen": "^10.11.0",
    "jsii-pacmak": "^1.103.1",
    "jsii-rosetta": "~5.9.0",
    "prettier": "^3.2.5",
    "projen": "^0.81.15",
    "standard-version": "^9",
    "ts-jest": "^27",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "cdktf": "0.20.8",
    "constructs": "^10.3.0"
  },
  "dependencies": {
    "cdktf": "0.20.8"
  },
  "keywords": [
    "azapi",
    "azure",
    "cdk",
    "cdktf",
    "cloud",
    "devops",
    "infrastructure",
    "rest-api",
    "terraform"
  ],
  "engines": {
    "node": ">= 20.10.0"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(|src)/**/*(*.)@(spec|test|integ).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {},
    "setupFilesAfterEnv": [
      "<rootDir>/setup.js"
    ],
    "maxWorkers": "100%",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.microsoft.terraformcdkconstructs",
        "maven": {
          "groupId": "com.microsoft.terraformcdkconstructs",
          "artifactId": "cdktf-azure-constructs"
        }
      },
      "python": {
        "distName": "microsoft-cdktfconstructs",
        "module": "microsoft_cdktfconstructs"
      },
      "dotnet": {
        "namespace": "Microsoft.Cdktf.Azure.TFConstructs",
        "packageId": "Microsoft.Cdktf.Azure.TFConstructs"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
